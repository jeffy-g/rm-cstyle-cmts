{
  "name": "rm-cstyle-cmts",
  "version": "3.3.26",
  "description": "remove c style comments from text file(javascript source, json file etc...",
  "private": false,
  "main": "./cjs/index.js",
  "types": "./index.d.ts",
  "unpkg": "./umd/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./cjs/index.js",
      "require": "./cjs/index.js",
      "types": "./index.d.ts"
    },
    "./gulp": {
      "import": "./cjs/gulp/index.js",
      "require": "./cjs/gulp/index.js",
      "types": "./cjs/gulp/index.d.ts"
    },
    "./webpack": {
      "import": "./webpack/index.js",
      "require": "./webpack/index.js",
      "types": "./index.d.ts"
    },
    "./webpack/gulp": {
      "import": "./webpack/gulp/index.js",
      "require": "./webpack/gulp/index.js",
      "types": "./webpack/gulp/index.d.ts"
    },
    "./umd": {
      "import": "./umd/index.js",
      "require": "./umd/index.js",
      "types": "./index.d.ts"
    }
  },
  "author": "jeffy-g",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffy-g/rm-cstyle-cmts/issues"
  },
  "homepage": "https://github.com/jeffy-g/rm-cstyle-cmts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffy-g/rm-cstyle-cmts.git"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "cjs",
    "umd",
    "webpack",
    "*.d.ts"
  ],
  "keywords": [
    "typescript",
    "javascript",
    "remove",
    "delete",
    "comment",
    "comments",
    "strip",
    "json",
    "text"
  ],
  "engines": {
    "node": ">=12"
  },
  "defs": {
    "projectname": "rm-cstyle-cmts"
  },
  "scripts": {
    "all": "yarn clean && eslint && yarn dist && yarn webpack && yarn copy:types && yarn preserve:jsdoc",
    "bench:empty": "node -v && node ./build/cjs/bench/ -empty -l 5e7 -ol 10 | node ./build/cjs/bench/ -p",
    "bench": "node -v && node ./build/cjs/bench/ -f samples/es6.js -l 2000 -ol 10 | node ./build/cjs/bench/ -p",
    "benchmark": "cd etc/bench && yarn install && yarn start",
    "build:test": "tsc -m commonjs -t es2019 __tests__/test.ts",
    "build": "tsc && yarn fire:tool fix_import_path",
    "clean": "rimraf -g build/* dist/* logs/*.tsbuildinfo",
    "copy:files": "yarn fire:tool copyfiles",
    "copy:types": "yarn fire:tool copytypes",
    "dist": "rimraf -g \"dist/!(webpack|umd)\" && yarn build && yarn copy:files && orgpkg -p -k defs",
    "fire:tool": "bash scripts/tool.sh",
    "grmc-test:cjs": "node scripts/batch-rmc-test.js -progress -collectRegex -showNoops -timeMeasure -collectJSDocTag",
    "ic": "rimraf yarn.lock node_modules && yarn install",
    "lint": "eslint",
    "patch:tag": "yarn fire:tool patch_with_tag",
    "preserve:jsdoc": "yarn fire:tool del_usestrict && jstool -cmd rmc -rmc4ts -basePath \"./dist/cjs,./dist/cjs/gulp\" -test \"/\\.(js|d\\.ts)$/\"",
    "pretest": "rimraf -g ./coverage/*",
    "test": "bash -c \"[ -z $JEST_CONF ] && JEST_CONF=jest.config.js; jest --silent=false -c=$JEST_CONF\"",
    "toggle:cmts": "jstool -cmd cmtTrick -targets \"js-scanner.ts,\" -basePath src",
    "ts:check": "tsc --noemit",
    "up": "yarn upgrade-interactive --latest",
    "v": "jstool -cmd version -extras \"./src/index.ts,\"",
    "webpack": "yarn fire:tool webpack",
    "git:push": "yarn fire:tool force_push",
    "postgit:push": "git gc --aggressive --prune=now",
    "strip:strict": "jstool -cmd replace -regex \"/.use strict.;\\s/g\" -targets \"dist/cjs/index.js,dist/cjs/js-scanner.js,dist/cjs/gulp/index.js\""
  },
  "//": [
    "NOTE: `strip:strict` use for publish (dist)",
    "ESLint JavaScript Plugin(rules): @eslint/js"
  ],
  "devDependencies": {
    "@jeffy-g/cpx": "^1.6.7",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/gulp": "^4.0.18",
    "@types/jest": "^30.0.0",
    "@types/vinyl": "^2.0.12",
    "@typescript-eslint/parser": "^8.52.0",
    "colors.ts": "^1.0.20",
    "eslint": "^9.39.2",
    "gulp": "^5.0.1",
    "jest": "^30.2.0",
    "js-dev-tool": "^1.2.0",
    "organize-pkg": "^1.3.2",
    "replace": "^1.2.2",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
