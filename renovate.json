{
    "extends": [
        "config:base", "schedule:daily"
    ],
    "statusCheckVerify": true,
    "ignoreDeps": [
        "through2", "eslint",
        "@typescript-eslint/eslint-plugin",
        "@typescript-eslint/parser"
    ],
    "automerge": true,
    "automergeType": "branch",
    "rangeStrategy": "replace",
    "minor": {
        "groupName": "all dependencies"
    },
    "commitBody": "[skip ci]"
}
